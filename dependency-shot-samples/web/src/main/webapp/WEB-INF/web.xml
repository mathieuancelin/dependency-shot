<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>VDM</display-name>
    <listener>
        <description>ServletContextListener</description>
        <listener-class>cx.ath.mancel01.dependencyshot.samples.vdm.web.DependencyShotListener</listener-class>
    </listener>
    <context-param>
        <param-name>DependencyShotConfig</param-name>
        <param-value>cx.ath.mancel01.dependencyshot.samples.vdm.config.VdmServiceBinder</param-value>
    </context-param>
    <welcome-file-list>
        <welcome-file>vdm.jsp</welcome-file>
    </welcome-file-list>
</web-app>
